                                    areaStyle: {
                                        type: 'default'
                                    }
                                }
                            },
                            data: yData2
                        }
                    ]
                };
                //console.log(option);
                setTimeout(function () {
                    chart3 = echarts.init(document.getElementById('chart3'));
                    chart3.clear();
                    chart3.setOption(option, true);
                }, 500);
            }
        });
