        var dataMap5 = {}; // RTS_CONSUMER Map
        var befDatetime = new Date(new Date(datetime).getTime() - interval * 60000);
        //var name = "时间（分）";
        var milli = 60000;
        var format = "yyyy-MM-dd HH:mm";
