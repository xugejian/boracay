<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration >
    <classPathEntry location="../lib/jdbc/ojdbc6.jar" />

    <context id="context1"  targetRuntime="MyBatis3Simple"><!--&lt;!&ndash;  targetRuntime="MyBatis3Simple" &ndash;&gt;-->
        <commentGenerator>
            <property name="suppressAllComments" value="true" />
            <property name="suppressDate" value="true" />
        </commentGenerator>

        <jdbcConnection driverClass="oracle.jdbc.OracleDriver" connectionURL="jdbc:oracle:thin:@//10.1.97.3:1521/hadoop" userId="udsp" password="udsp" />
        <javaTypeResolver >
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>
        <javaModelGenerator targetPackage="com.hex.bigdata.udsp.mm.model" targetProject="src">
            <property name="enableSubPackages" value="true" />
            <property name="trimStrings" value="false" />
        </javaModelGenerator>
        <sqlMapGenerator targetPackage="goframe.mm.mapper.general" targetProject="resources">
            <property name="enableSubPackages" value="true" />
        </sqlMapGenerator>
        <javaClientGenerator targetPackage="com.hex.bigdata.udsp.mm.dao" targetProject="src" type="XMLMAPPER">
            <property name="enableSubPackages" value="true" />
        </javaClientGenerator>

        <table tableName="MM_APPLICATION" domainObjectName="MmApplication"/>
        <table tableName="MM_APPLICATION_EXECUTE_PARAM" domainObjectName="MmAppExecuteParam"/>
        <table tableName="MM_APPLICATION_RETURN_PARAM" domainObjectName="MmAppReturnParam"/>
        <table tableName="MM_CONTRACTOR" domainObjectName="MmContractor"/>
        <table tableName="MM_MODEL_INFO" domainObjectName="MmModelInfo"/>
        <table tableName="MM_MODEL_PARAM" domainObjectName="MmModelParam"/>
  </context>
</generatorConfiguration>