    //获取目标元数据字段
    function getTargetCols() {
        $.ajax({
            url: "${basePath}/im/md/col/select/" + targetSourceId,
            type: 'POST',
            cache: false,
            async: false,
            contentType: 'application/json',
            success: function (data) {
