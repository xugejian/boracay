                targetSourceId = action;

                //获取目标元数据可选字段
                getTargetCols();
                var data = grid2.getData(true, true);
                if (data != null && data.length > 0) {
                    autoMapping(data)
                    grid3.addRows($.extend(true, [], mappingTargetData));
                }
            }
        })
    }

