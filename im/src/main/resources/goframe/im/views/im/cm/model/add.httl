                    cache: false,
                    async: false,
                    contentType: 'application/json',
                    success: function (data) {
                        targetDsModel.load(data.data);
                    }
                });
                var k = 0;
                for (; k < targetDsModel.data.length; k++) {
                    if (targetDsModel.data[k].pkId == action) {
                        break;
                    }
